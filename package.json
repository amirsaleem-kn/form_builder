{
  "name": "survey",
  "version": "1.0.0",
  "description": "Create dynamic forms for your survey",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "build": "tsc",
    "generate-docs": "./node_modules/.bin/typedoc --theme node_modules/typedoc-twilio-theme/bin --out docs ./src",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "prestart": "npm run build",
    "start": "nodemon .",
    "test": "./node_modules/.bin/mocha --timeout 50000 -r ts-node/register src/**/*.spec.ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon ./dist/server.js",
    "watch-ts": "tsc -w"
  },
  "keywords": [
    "survey",
    "form",
    "builder",
    "dynamic",
    "forms"
  ],
  "author": "Amir Saleem",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.1.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.6",
    "@types/mongoose-unique-validator": "^1.0.1",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.0.4",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.2",
    "link-module-alias": "^1.2.0",
    "mocha": "^6.1.4",
    "node-mocks-http": "^1.7.6",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typedoc-twilio-theme": "^1.0.0",
    "typedoc-vortex-theme": "0.0.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "express-jwt-authz": "^2.3.1",
    "express-validator": "^5.3.1",
    "handlebars": "^4.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.2",
    "mongoose-unique-validator": "^2.0.3",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.17"
  }
}
