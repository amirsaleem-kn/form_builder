{
  "name": "survey",
  "version": "1.0.0",
  "description": "Create dynamic forms for your survey",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run test",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "watch-node": "nodemon ./dist/server.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "prestart": "npm run build",
    "start": "nodemon ."
  },
  "keywords": [
    "survey",
    "form",
    "builder",
    "dynamic",
    "forms"
  ],
  "author": "Amir Saleem",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.0.4",
    "concurrently": "^4.1.0",
    "link-module-alias": "^1.2.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.17"
  }
}
